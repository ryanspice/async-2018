{
	"name": "async.2018",
	"short_name": "async2018",
	"level": "info",
	"version": "1.2.0-beta.2",
	"description": "",
	"scripts": {
		"flow-check": "flow --color=always",
		"start": "yarn serve",
		"build": "yarn clean:dist && yarn dev",
		"build:dev": "yarn build",
		"build:prod": "yarn prod",
		"assets": "yarn parallel:assets && yarn docs",
		"docs": "yarn esdoc",
		"clean:all": "yarn clean:dist && yarn clean:lib",
		"clean:install": "yarn rimraf node_modules",
		"clean:dist": "yarn rimraf dist",
		"clean:lib": "yarn rimraf lib",
		"serve": "yarn webpack-dev-server --config ./config/webpack.config.js --env.NODE_ENV=development --env.development",
		"server": "yarn parallel:assets && yarn webpack-dev-server --config ./config/webpack.config.js --env.NODE_ENV=development --env.development",
		"serve:dev": "yarn serve",
		"serve:prod": "yarn preview",
		"dev": "yarn clean:dist && yarn parallel:assets && yarn parallel:dev",
		"prod": "yarn clean:dist && yarn parallel:assets && yarn parallel:prod",
		"deploy": "yarn clean:all && yarn parallel:assets && yarn docs && yarn parallel --no-open && yarn library",
		"deployWithoutDocs": "yarn clean:all && yarn parallel:assets && yarn parallel --no-open && yarn library",
		"watch": "yarn webpack --config=./config/webpack.config.js --env.NODE_ENV=development --env.development --watch",
		"parallel": "yarn parallel:prod",
		"parallel:stats": "yarn webpack --config=./config/webpack.prod2.js --env.NODE_ENV=development --env.development --max_old_space_size=4096 --profile --json > ./dist/stats.json && node ./config/script.clean.json.js",
		"parallel:dev": "yarn parallel-webpack --config=./config/webpack.config2.js --env.NODE_ENV=development --env.development --max_old_space_size=4096",
		"parallel:prod": "yarn parallel-webpack --config=./config/webpack.prod2.js --env.NODE_ENV=production --env.production",
		"parallel:assets": "yarn parallel-webpack --config=./config/webpack.assets.js --env.NODE_ENV=development --env.development",
		"preview": "yarn parallel:assets && yarn webpack-dev-server --config ./config/webpack.prod.js --env.NODE_ENV=production --env.production",
		"library": "yarn clean:lib && node ./config/script.minify.js",
		"dashboard": "yarn webpack-dashboard yarn prod --watch",
		"release:npm": "npm run deploy && npm publish ."
	},
	"author": "Ryan Tyler Spice-Finnie <spice.ryan@hotmail.com>",
	"private": true,
	"license": "UNLICENSED",
	"dependencies": {
		"copy-webpack-plugin": "^5.0.3",
		"copyfiles": "^2.1.0",
		"css-loader": "2.0.1",
		"cssnano": "^4.1.7",
		"esdoc": "^1.1.0",
		"esdoc-accessor-plugin": "^1.0.0",
		"esdoc-babel-plugin": "ryanspice/esdoc-babel-plugin",
		"esdoc-brand-plugin": "^1.0.1",
		"esdoc-ecmascript-proposal-plugin": "^1.0.0",
		"esdoc-external-ecmascript-plugin": "^1.0.0",
		"esdoc-integrate-manual-plugin": "^1.0.0",
		"esdoc-standard-plugin": "^1.0.0",
		"esdoc-type-inference-plugin": "^1.0.2",
		"esdoc-undocumented-identifier-plugin": "^1.0.0",
		"esdoc-unexported-identifier-plugin": "^1.0.0",
		"flexible-pwa-webpack-plugin": "^0.0.10",
		"glob-all": "^3.1.0",
		"html-webpack-externals-plugin": "^3.8.0",
		"html-webpack-plugin": "https://github.com/PutziSan/html-webpack-plugin.git",
		"http-server": "^0.11.1",
		"json-loader": "^0.5.6",
		"mini-css-extract-plugin": "0.6.0",
		"node-sass": "^4.10.0",
		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"parallel-webpack": "^2.3.0",
		"postcss-import": "^12.0.1",
		"postcss-loader": "^3.0.0",
		"postcss-preset-env": "^6.4.0",
		"purify-css": "^1.2.5",
		"purifycss-webpack": "^0.7.0",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.1",
		"sugarss": "^2.0.0"
	},
	"devDependencies": {
		"basic-ftp":"^3.8.0",
		"fs":"latest",
		"ecmascript-toolkit": "https://github.com/ryanspice/ecmascript-toolkit.git#master"
	}
}
