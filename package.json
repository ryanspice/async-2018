		"circular-dependency-plugin": "^5.0.2",
		"copy-webpack-plugin": "^5.0.3",
		"css-loader": "2.0.1",
		"cssnano": "^4.1.7",
		"duplicate-package-checker-webpack-plugin": "^3.0.0",
		"esdoc": "^1.1.0",
		"esdoc-accessor-plugin": "^1.0.0",
		"esdoc-babel-plugin": "ryanspice/esdoc-babel-plugin",
