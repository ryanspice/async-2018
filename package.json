{
	"name": "async.2018",
	"version": "1.0.0",
	"description": "",
	"private": true,
	"author": "Ryan Tyler Spice-Finnie",
	"scripts": {
		"boom": "sudo rm -rf node_modules && yarn && yarn dev",
		"dev": "yarn webpack-dev-server",
		"watch": "yarn webpack --watch",
		"dash": "yarn webpack-dashboard -- webpack-dev-server",
		"prod": "yarn webpack --config webpack.prod.js"
	},
	"dependencies": {
		"@babel/core": "^7.0.0",
		"@babel/plugin-proposal-class-properties": "^7.0.0",
		"@babel/plugin-proposal-decorators": "^7.0.0",
		"@babel/plugin-proposal-export-namespace-from": "^7.0.0",
		"@babel/plugin-proposal-function-sent": "^7.0.0",
		"@babel/plugin-proposal-json-strings": "^7.0.0",
		"@babel/plugin-proposal-numeric-separator": "^7.0.0",
		"@babel/plugin-proposal-throw-expressions": "^7.0.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/plugin-syntax-import-meta": "^7.0.0",
		"@babel/plugin-transform-flow-strip-types": "^7.0.0",
		"@babel/plugin-transform-runtime": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/preset-flow": "^7.0.0",
		"@babel/preset-stage-2": "^7.0.0",
		"@babel/runtime": "^7.0.0",
		"babel-loader": "^8.0.0",
		"babel-plugin-flow-runtime": "^0.17.0",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-upgrade": "^0.0.23",
		"copy-webpack-plugin": "^4.5.1",
		"css-loader": "^0.28.11",
		"flow-bin": "^0.71.0",
		"flow-runtime": "^0.17.0",
		"flow-runtime-cli": "^0.17.0",
