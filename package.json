		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.1",
		"sugarss": "^2.0.0",
		"webpack": "^4.32.2",
		"webpack-bundle-analyzer": "^3.3.2",
		"webpack-cli": "^3.3.2",
		"webpack-deadcode-plugin": "^0.1.10",
		"webpack-dev-server": "^3.4.1",
		"webpack-manifest-plugin": "^2.0.4",
		"webpack-merge": "^4.1.2",
		"webpack-polyfill-injector": "^2.0.2",
